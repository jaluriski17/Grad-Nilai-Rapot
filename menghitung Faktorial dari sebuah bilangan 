using System;

class Program
{
    static void Main()
    {
        Console.Write("Masukkan bilangan untuk menghitung faktorial: ");
        int bilangan = Convert.ToInt32(Console.ReadLine());

        long hasilFaktorial = HitungFaktorial(bilangan);

        Console.WriteLine($"Faktorial dari {bilangan} adalah: {hasilFaktorial}");
    }

    static long HitungFaktorial(int n)
    {
        if (n == 0 || n == 1)
        {
            return 1;
        }
        else
        {
            long hasil = 1;
            for (int i = 2; i <= n; i++)
            {
                hasil *= i;
            }
            return hasil;
        }
    }
}
